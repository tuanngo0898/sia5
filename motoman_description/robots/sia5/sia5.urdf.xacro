<?xml version="1.0"?>
<robot name="motoman_sia5" xmlns:xacro="http://ros.org/wiki/xacro">
  <!-- File Include -->
  <xacro:include filename="$(find motoman_description)/robots/sia5/sia5_basic.urdf.xacro" />

  <xacro:arg name="x" default="0" />
  <xacro:arg name="y" default="0" />
  <xacro:arg name="z" default="0" />

  <xacro:add_world>
    <origin xyz="$(arg x) $(arg y) $(arg z)" rpy="0 0 0"/>
  </xacro:add_world>

</robot>
